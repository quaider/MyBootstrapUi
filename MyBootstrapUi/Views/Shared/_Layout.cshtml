<!DOCTYPE html>
<html class="dark-theme">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - my bootstrap ui</title>
    @Styles.Render("~/Content/css")
</head>
<body>
    <!--begin page-->
    <div id="wrapper" @*class="sidebar-collapsed"*@>
        <!--topbar start-->
        <div class="topbar">
            <!--logo-->
            <div class="topbar-left">
                <a href="javascript:void(0)" class="logo">
                    <span>
                        Admin
                        <span>to</span>
                    </span>
                </a>
            </div>
            <!--Navbar-->
            <div class="navbar navbar-default">
                <div class="container">
                    <!--page title-->
                    <ul class="nav navbar-nav pull-left">
                        <li class="nav-item">
                            <button class="navbar-minimize">
                                <i class="fa fa-bars"></i>
                            </button>
                        </li>
                        <li class="nav-item">
                            <h4 class="page-title">Dashboard</h4>
                        </li>
                    </ul>
                    <ul class="nav navbar-nav pull-right">
                        <li class="nav-item">
                            <a href="javascript:void(0)" class="nav-link">
                                <span class="nav-icon">
                                    <i class="fa fa-th"></i>
                                </span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="javascript:void(0)" class="nav-link">
                                <span class="badge badge-attension">6</span>
                                <span class="nav-icon">
                                    <i class="fa fa-weixin"></i>
                                </span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="javascript:void(0)" class="nav-link">
                                <span class="nav-icon">
                                    <i class="fa fa-expand"></i>
                                </span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!--left side menu-->
        <div class="left side-menu">
            <div class="sidebar-inner slimscrollleft">
                <div id="sidebar-menu">
                    <ul>
                        <li class="text-muted menu-title">菜单导航</li>
                        <li>
                            <a href="javascript:void(0)" class="waves-effect">
                                <i class="fa fa-tachometer "></i>
                                <span>Dashboard</span>
                            </a>
                        </li>
                        <li class="has-sub">
                            <a href="javascript:void(0)" class="waves-effect">
                                <i class="fa fa-expand"></i>
                                <span>Layouts</span>
                                <span class="menu-arrow"></span>
                            </a>
                            <ul class="nav">
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Test 1</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Test 2</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="has-sub">
                            <a href="javascript:void(0)" class="waves-effect">
                                <i class="fa fa-firefox"></i>
                                <span>UI Elements</span>
                                <span class="menu-arrow"></span>
                            </a>
                            <ul class="nav">
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Panels</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Buttons</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Typography</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Modals</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Alert</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Tooltips</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Popovers</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Breadcrumbs</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Progress Bars</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Pagination</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Video</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a href="javascript:void(0)" class="waves-effect">
                                <i class="fa fa-envelope"></i>
                                <span>Email</span>
                            </a>
                        </li>
                        <li>
                            <a href="javascript:void(0)" class="waves-effect">
                                <i class="fa fa-area-chart"></i>
                                <span>Charts</span>
                            </a>
                        </li>
                        <!--start menu levels-->
                        <li class="has-sub">
                            <a href="javascript:void(0)" class="waves-effect">
                                <i class="fa fa-sitemap"></i>
                                <span>Menu Levels</span>
                                <span class="menu-arrow"></span>
                            </a>
                            <ul class="nav">
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Menu Level 1.1</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        <span class="title">Menu Level 1.2</span>
                                    </a>
                                </li>
                                <li class="has-sub">
                                    <a href="javascript:void(0)">
                                        <span class="title">Menu Level 1.3</span>
                                        <span class="menu-arrow"></span>
                                    </a>
                                    <ul class="nav">
                                        <li>
                                            <a href="javascript:void(0)">
                                                <span class="title">Menu Level 2.1</span>
                                            </a>
                                        </li>
                                        <li class="has-sub">
                                            <a href="javascript:void(0)">
                                                <span class="title">Menu Level 2.2</span>
                                                <span class="menu-arrow"></span>
                                            </a>
                                            <ul class="nav">
                                                <li class="has-sub">
                                                    <a href="javascript:void(0)">
                                                        <span class="title">Menu Level 3.1</span>
                                                        <span class="menu-arrow"></span>
                                                    </a>
                                                    <ul class="nav">
                                                        <li>
                                                            <a href="javascript:void(0)">
                                                                <span class="title">Menu Level 4.1</span>
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </li>
                                                <li>
                                                    <a href="javascript:void(0)">
                                                        <span class="title">Menu Level 3.2</span>
                                                    </a>
                                                </li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a href="javascript:void(0)">
                                                <span class="title">Menu Level 2.3</span>
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
        <!--main content-->
        <div class="content-page">
            <!--start content-->
            <div class="content">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card-box">
                                <h4 class="header-title m-t-0 m-b-30">Input Types</h4>
                                <div class="row">
                                    @RenderBody()
                                </div>
                                <!-- end row -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--start foot-->
            <footer class="footer">
                2016 © Adminto.
            </footer>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/datatable")
    <script src="~/Scripts/quaider.menu.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
