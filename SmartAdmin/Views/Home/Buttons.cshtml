</div>
<div class="row">
    <div class="col-md-4">
        <div class="panel panel-blank">
            <div class="panel-heading">
                <h3 class="panel-title">Button Sizes</h3>
            </div>
            <div class="panel-body">
                <button class="btn btn-primary btn-lg">Large button</button>
                <button class="btn btn-success">Normal button</button>
                <button class="btn btn-danger btn-sm">Small button</button>
                <button class="btn btn-lilac btn-xs">Extra small button</button>
            </div>
        </div>
    </div>
    <div class="col-md-8">
        <div class="panel panel-blank">
            <div class="panel-heading">
                <h3 class="panel-title">Button Disabled</h3>
            </div>
            <div class="panel-body">
                <button class="btn btn-default disabled">default</button>
                <button class="btn btn-primary disabled">primary</button>
                <button class="btn btn-success disabled">success</button>
                <button class="btn btn-info disabled">info</button>
                <button class="btn btn-warning disabled">warning</button>
                <button class="btn btn-danger disabled">danger</button>
                <button class="btn btn-inverse disabled">inverse</button>
                <button class="btn btn-lilac disabled">inverse</button>
                <button class="btn btn-teal disabled">inverse</button>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-4">
        <div class="panel panel-blank">
            <div class="panel-heading">
                <h3 class="panel-title">Icon Button</h3>
            </div>
            <div class="panel-body">
                <button class="btn btn-icon btn-default m-b-5"><i class="fa fa-heart-o"></i></button>
                <button class="btn btn-icon btn-danger m-b-5"><i class="fa fa-remove"></i></button>
                <button class="btn btn-icon btn-lilac m-b-5"><i class="fa fa-music"></i></button>
                <button class="btn btn-icon btn-primary m-b-5"><i class="fa fa-star"></i></button>
                <button class="btn btn-icon btn-success m-b-5"><i class="fa fa-thumbs-o-up"></i></button>
                <button class="btn btn-icon btn-info m-b-5"> <i class="fa fa-keyboard-o"></i></button>
                <button class="btn btn-icon btn-warning m-b-5"><i class="fa fa-wrench"></i></button>
                <button class="btn btn-icon btn-inverse m-b-5"><i class="fa fa-cog"></i></button>
                <button class="btn btn-icon btn-teal m-b-5"><i class="fa fa-check"></i></button>
            </div>
        </div>
    </div>
    <div class="col-md-8">
        <div class="panel panel-blank">
            <div class="panel-heading">
                <h3 class="panel-title">Icon Button With Text</h3>
            </div>
            <div class="panel-body">
                <button class="btn btn-icon btn-default m-b-5">
                    <i class="fa fa-heart-o"></i>
                    <span>喜欢</span>
                </button>
                <button class="btn btn-icon btn-danger m-b-5">
                    <i class="fa fa-remove"></i>
                    <span>删除</span>
                </button>
                <button class="btn btn-icon btn-lilac m-b-5">
                    <i class="fa fa-music"></i>
                    <span>音乐</span>
                </button>
                <button class="btn btn-icon btn-primary m-b-5">
                    <i class="fa fa-star"></i>
                    <span>收藏</span>
                </button>
                <button class="btn btn-icon btn-success m-b-5">
                    <i class="fa fa-thumbs-o-up"></i>
                    <span>点赞</span>
                </button>
                <button class="btn btn-icon btn-info m-b-5">
                    <i class="fa fa-keyboard-o"></i>
                    <span>输入法</span>
                </button>
                <button class="btn btn-icon btn-warning m-b-5">
                    <i class="fa fa-wrench"></i>
                    <span>修复</span>
                </button>
                <button class="btn btn-icon btn-inverse m-b-5">
                    <i class="fa fa-cog"></i>
                    <span>设置</span>
                </button>
                <button class="btn btn-icon btn-teal m-b-5">
                    <i class="fa fa-check"></i>
                    <span>提交</span>
                </button>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-blank">
            <div class="panel-heading">
                <h3 class="panel-title">Button Group</h3>
            </div>
            <div class="panel-body">
                <div class="btn-group m-b-10">
                    <button type="button" class="btn btn-default">Left</button>
                    <button type="button" class="btn btn-default">Middle</button>
                    <button type="button" class="btn btn-default">Right</button>
                </div>
                <div class="btn-group m-b-10">
                    <button type="button" class="btn btn-success">Left</button>
                    <button type="button" class="btn btn-success">Middle</button>
                    <button type="button" class="btn btn-success">Right</button>
                </div>
                <div class="btn-group m-b-10">
                    <button type="button" class="btn btn-primary">Left</button>
                    <button type="button" class="btn btn-primary">Middle</button>
                    <button type="button" class="btn btn-primary">Right</button>
                </div>
                <div class="btn-group m-b-10">
                    <button type="button" class="btn btn-info">Left</button>
                    <button type="button" class="btn btn-info">Middle</button>
                    <button type="button" class="btn btn-info">Right</button>
                </div>
                <div class="btn-group m-b-10">
                    <button type="button" class="btn btn-warning">Left</button>
                    <button type="button" class="btn btn-warning">Middle</button>
                    <button type="button" class="btn btn-warning">Right</button>
                </div>
                <div class="btn-group m-b-10">
                    <button type="button" class="btn btn-danger">Left</button>
                    <button type="button" class="btn btn-danger">Middle</button>
                    <button type="button" class="btn btn-danger">Right</button>
                </div>
                <div class="btn-group m-b-10">
                    <button type="button" class="btn btn-inverse">Left</button>
                    <button type="button" class="btn btn-inverse">Middle</button>
                    <button type="button" class="btn btn-inverse">Right</button>
                </div>
                <div class="btn-group m-b-10">
                    <button type="button" class="btn btn-lilac">Left</button>
                    <button type="button" class="btn btn-lilac">Middle</button>
                    <button type="button" class="btn btn-lilac">Right</button>
                </div>
                <div class="btn-group m-b-10">
                    <button type="button" class="btn btn-teal">Left</button>
                    <button type="button" class="btn btn-teal">Middle</button>
                    <button type="button" class="btn btn-teal">Right</button>
                </div>
            </div>
        </div>
    </div>
</div>